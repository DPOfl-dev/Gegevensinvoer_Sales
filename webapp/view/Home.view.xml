<mvc:View controllerName="com.Flexso.DPO.Gegevensinvoer_Sales.controller.Home" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<List items="{/d/results}">
							<items>
								<ObjectListItem title="{Name}"/></items>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>